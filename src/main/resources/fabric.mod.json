{
	"schemaVersion": 1,
	"id": "keep-chest",
	"version": "${version}",
	"name": "Keep Chest",
        "description": "Automatically stores a player's inventory inside a death chest when they die.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/keep-chest/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"chihalu.keepchest.KeepChest"
		],
		"client": [
			"chihalu.keepchest.KeepChestClient"
		],
		"fabric-datagen": [
			"chihalu.keepchest.KeepChestDataGenerator"
		]
	},
	"mixins": [
